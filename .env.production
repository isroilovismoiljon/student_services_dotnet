# Database Configuration
ConnectionStrings__DefaultConnection=Host=localhost;Database=TalabajonApp;Username=postgres;Password={{DB_PASSWORD}};Port=5432

# JWT Configuration
Jwt__SecretKey={{JWT_SECRET_KEY}}
Jwt__Issuer=StudentServicesWebApi
Jwt__Audience=StudentServicesWebApi-Users
Jwt__ExpiryInMinutes=14400
Jwt__RefreshTokenExpiryInDays=365

# Telegram Bot Configuration
TelegramBot__BotToken={{TELEGRAM_BOT_TOKEN}}
TelegramBot__BotUsername=Talabajon_services_bot
TelegramBot__BaseUrl=https://t.me

# ASP.NET Core Configuration
ASPNETCORE_ENVIRONMENT=Production
ASPNETCORE_URLS=http://localhost:5000
